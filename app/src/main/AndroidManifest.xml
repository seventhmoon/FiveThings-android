<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidfung.fivethings">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:label="Page 1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapsIntentActivity" android:label="Page 2"/>
        <activity android:name=".MailIntentActivity" android:label="Page 3"/>
        <activity android:name=".HttpIntentActivity" android:label="Page 4"/>
        <activity android:name=".MarketIntentActivity" android:label="Page 5"></activity>
    </application>

</manifest>